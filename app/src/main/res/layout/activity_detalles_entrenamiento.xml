<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="8dp"
    tools:context=".Entrenador.Entrenamientos.DetallesEntrenamiento">

    <VideoView
        android:id="@+id/videoEntrenamiento"
        android:layout_width="300dp"
        android:layout_height="300dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tituloEntrenamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Titulo entrenamiento"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/videoEntrenamiento"
        app:layout_constraintVertical_bias="0.036" />

    <TextView
        android:id="@+id/descripcionEntrenamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descripcion entrenamiento"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tituloEntrenamiento"
        app:layout_constraintVertical_bias="0.028" />

    <TextView
        android:id="@+id/categoriaEntrenamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="edades"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.163"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/descripcionEntrenamiento"
        app:layout_constraintVertical_bias="0.063" />

    <TextView
        android:id="@+id/dificultadEntrenamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dificultad"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.277"
        app:layout_constraintStart_toEndOf="@+id/categoriaEntrenamiento"
        app:layout_constraintTop_toBottomOf="@+id/descripcionEntrenamiento"
        app:layout_constraintVertical_bias="0.063" />

    <TextView
        android:id="@+id/duracionEntrenamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="duracion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toEndOf="@+id/dificultadEntrenamiento"
        app:layout_constraintTop_toBottomOf="@+id/descripcionEntrenamiento"
        app:layout_constraintVertical_bias="0.063" />

    <TextView
        android:id="@+id/materialesEntrenamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="materiales"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.305"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dificultadEntrenamiento"
        app:layout_constraintVertical_bias="0.086" />

    <TextView
        android:id="@+id/tipoEntrenamiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tipo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.347"
        app:layout_constraintStart_toEndOf="@+id/materialesEntrenamiento"
        app:layout_constraintTop_toBottomOf="@+id/duracionEntrenamiento"
        app:layout_constraintVertical_bias="0.089" />
</androidx.constraintlayout.widget.ConstraintLayout>